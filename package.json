{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "sass-watch": "node-sass --include-path scss scss/style.scss public/stylesheets/style.css --output-style=compressed -w",
    "browser-sync": "browser-sync start --proxy localhost:3000 --files views,lang,public",
    "dev": "concurrently \"npm start\" \"npm run sass-watch\" \"npm run browser-sync\""
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-rate-limit": "^2.7.0",
    "morgan": "~1.8.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "browser-sync": "^2.18.8",
    "concurrently": "^3.4.0",
    "connect-browser-sync": "^2.1.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.5.2"
  }
}
